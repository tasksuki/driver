{
    "name": "tasksuki/driver",
    "type": "library",
    "description": "Tasksuki Driver Component",
    "keywords": ["driver", "tasksuki"],
    "homepage": "https://tasksuki.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": { "Tasksuki\\Component\\Driver\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Tasksuki\\Component\\Driver\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
